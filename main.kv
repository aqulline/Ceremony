#:import sm kivy.uix.screenmanager
ScreenManager:
    id:manager
    transition:sm.FadeTransition(duration=.0)
    MDScreen:
        name: "login"
        MDFloatLayout:
            orientation: "vertical"
            md_bg_color: "#EAF4F4"  # Soft background color for a clean look

            # Company/Welcome Text
            MDLabel:
                text: "Welcome Back!"
                size_hint_y: None
                height: dp(60)
                font_size: "28sp"
                bold: True
                halign: "center"
                color: "#0B4F6C"
                pos_hint: {"center_x": .5, "center_y": .85}

            # Logo Image Placeholder (Add logo here)
            MDIcon:
                icon: "account-circle"  # Replace with your logo
                size_hint: None, None
                size: dp(100), dp(100)
                pos_hint: {"center_x": .5, "center_y": .75}
                theme_text_color: "Custom"
                text_color: "#0B4F6C"  # Matching primary color

            # Phone Number Label
            MDLabel:
                text: "Customer Number"
                size_hint_y: None
                height: self.texture_size[1]
                halign: "center"
                font_size: "18sp"
                color: "#474747"
                pos_hint: {"center_x": .27, "center_y": .64}

            # Phone Number Input Field
            NumberOnlyField:
                id: user_number
                mode: "rectangle"
                hint_text: "Enter phone number"
                size_hint_x: .85
                pos_hint: {"center_x": .5, "center_y": .57}
                font_size: dp(18)
                foreground_color: "#353B3C"
                line_color_focus: "#0B4F6C"
                fill_color_focus: "#D9E4EC"
                hint_text_color_focus: "#0B4F6C"
                text_color_focus: "#0B4F6C"
                halign: "left"
                radius: [20, 20, 20, 20]  # Rounded corners

            # Password Label
            MDLabel:
                text: "Password"
                size_hint_y: None
                height: self.texture_size[1]
                halign: "center"
                font_size: "18sp"
                color: "#474747"
                pos_hint: {"center_x": .2, "center_y": .48}

            # Password Input Field
            MDTextField:
                id: password
                mode: "rectangle"
                hint_text: "Enter password"
                size_hint_x: .85
                pos_hint: {"center_x": .5, "center_y": .42}
                password: True
                font_size: dp(18)
                foreground_color: "#353B3C"
                line_color_focus: "#0B4F6C"
                fill_color_focus: "#D9E4EC"
                hint_text_color_focus: "#0B4F6C"
                text_color_focus: "#0B4F6C"
                halign: "left"
                radius: [20, 20, 20, 20]  # Rounded corners

            # Submit Button
            MDRaisedButton:
                text: "Log In"
                md_bg_color: "#0B4F6C"
                text_color: "#FFFFFF"
                size_hint: .5, .08
                pos_hint: {"center_x": .5, "center_y": .2}
                font_size: "18sp"
                radius: [25, 25, 25, 25]
                on_release:
                    app.user_login_opt()

            # Additional Buttons/Options (Forgot Password, Sign-up)
            MDTextButton:
                text: "Forgot password?"
                pos_hint: {"center_x": .5, "center_y": .1}
                theme_text_color: "Custom"
                text_color: "#0B4F6C"
                on_release:
                    app.show_forgot_password_screen()

            MDTextButton:
                text: "Sign up"
                pos_hint: {"center_x": .5, "center_y": .05}
                theme_text_color: "Custom"
                text_color: "#0B4F6C"
                on_release:
                    app.screen_capture('signup')

    MDScreen:
        name: "signup"
        MDFloatLayout:
            md_bg_color: "#EAF4F4"  # Soft background color for a clean look

            # Title of the Signup Screen
            MDLabel:
                text: "Create Your Account"
                font_style: "H5"
                halign: "center"
                bold: True
                color: "#0B4F6C"
                pos_hint: {"center_x": .5, "center_y": .9}

            # Name Field
            MDTextField:
                id: name_field
                hint_text: "Full Name"
                helper_text: "Please enter your full name"
                helper_text_mode: "on_focus"
                mode: "rectangle"
                icon_right: "account-outline"
                pos_hint: {"center_x": .5, "center_y": .75}
                size_hint_x: 0.85
                text_color: "#0B4F6C"
                font_size: "16sp"
                line_color_normal: "#BCCCDC"
                line_color_focus: "#0B4F6C"
                icon_right_color: "#BCCCDC"

            # Phone Number Field
            MDTextField:
                id: phone_field
                hint_text: "Phone Number"
                helper_text: "Enter a valid phone number"
                helper_text_mode: "on_focus"
                mode: "rectangle"
                icon_right: "phone-outline"
                pos_hint: {"center_x": .5, "center_y": .65}
                size_hint_x: 0.85
                text_color: "#0B4F6C"
                font_size: "16sp"
                line_color_normal: "#BCCCDC"
                line_color_focus: "#0B4F6C"
                icon_right_color: "#BCCCDC"
                input_filter: "int"  # Restrict to numeric input

            # Password Field
            MDTextField:
                id: password_field
                hint_text: "Password"
                helper_text: "Must be at least 8 characters"
                helper_text_mode: "on_focus"
                mode: "rectangle"
                password: True  # Makes input hidden for password
                icon_right: "eye-off-outline"  # Password visibility toggle
                pos_hint: {"center_x": .5, "center_y": .55}
                size_hint_x: 0.85
                text_color: "#0B4F6C"
                font_size: "16sp"
                line_color_normal: "#BCCCDC"
                line_color_focus: "#0B4F6C"
                icon_right_color: "#BCCCDC"

            # Submit Button with improved styling and shadow
            MDRaisedButton:
                text: "Create Account"
                md_bg_color: "#0B4F6C"
                text_color: "#FFFFFF"
                pos_hint: {"center_x": .5, "center_y": .35}
                size_hint_x: 0.7
                shadow_radius: 10  # Added shadow for depth effect
                on_release:
                    app.validate_signup(name_field.text, phone_field.text, password_field.text)

            # Separator
            MDSeparator:
                height: dp(1)
                pos_hint: {"center_x": .5, "center_y": .28}
                color: "#BCCCDC"

            # Link to Login Page
            MDTextButton:
                text: "Already have an account? Log in"
                pos_hint: {"center_x": .5, "center_y": .2}
                text_color: "#0B4F6C"
                on_release:
                    app.go_to_login()

            # Social Media Signup Option (Optional)
            MDLabel:
                text: "Or sign up with"
                halign: "center"
                font_size: "14sp"
                color: "#474747"
                pos_hint: {"center_x": .5, "center_y": .15}



    MDScreen:
        name: "otp_verification"
        MDFloatLayout:
            md_bg_color: "#EAF4F4"  # Soft background color for a clean look

            # Title for OTP Verification Screen
            MDLabel:
                text: "Verify Your Identity"
                font_style: "H4"
                halign: "center"
                bold: True
                color: "#0B4F6C"
                pos_hint: {"center_x": .5, "center_y": .88}

            # Subtitle Text with Instructions
            MDLabel:
                text: "Enter the 6-digit code sent to your phone"
                halign: "center"
                font_size: "16sp"
                color: "#474747"
                pos_hint: {"center_x": .5, "center_y": .8}

            # OTP Input Field with Custom Styling
            MDTextField:
                id: otp_field
                hint_text: "6-digit OTP"
                helper_text: "Please enter the code"
                helper_text_mode: "on_focus"
                mode: "rectangle"
                max_text_length: 6
                icon_right: "shield-key-outline"
                input_filter: "int"  # Restrict to numeric input
                pos_hint: {"center_x": .5, "center_y": .68}
                size_hint_x: 0.7
                text_color: "#0B4F6C"
                font_size: "16sp"
                line_color_normal: "#BCCCDC"
                line_color_focus: "#0B4F6C"
                icon_right_color: "#BCCCDC"

            # Timer Label for Resend OTP Functionality
            MDLabel:
                id: timer_label
                text: "Resend OTP in 00:30"
                halign: "center"
                font_size: "14sp"
                color: "#d4af37"
                pos_hint: {"center_x": .5, "center_y": .6}
                bold: True

            # Submit Button to Validate OTP
            MDRaisedButton:
                text: "Verify OTP"
                md_bg_color: "#0B4F6C"
                text_color: "#FFFFFF"
                pos_hint: {"center_x": .5, "center_y": .5}
                size_hint_x: 0.7
                elevation: 5  # Adds a depth effect
                shadow_radius: 15
                on_release:
                    app.verify_otp_opt()

            # Resend OTP Button - Active only after timer ends
            MDFlatButton:
                text: "Resend OTP"
                text_color: "#0B4F6C"
                bold: True
                pos_hint: {"center_x": .5, "center_y": .42}
                on_release:
                    app.resend_otp()

            # Go Back to Login Button
            MDTextButton:
                text: "Back to Login"
                pos_hint: {"center_x": .5, "center_y": .3}
                text_color: "#0B4F6C"
                on_release:
                    app.go_to_login()

            # Separator for clean sectioning
            MDSeparator:
                height: dp(1)
                color: "#BCCCDC"
                pos_hint: {"center_x": .5, "center_y": .25}

            # Additional Text for Support or Help
            MDLabel:
                text: "Need help? Contact support"
                halign: "center"
                font_size: "14sp"
                color: "#474747"
                pos_hint: {"center_x": .5, "center_y": .18}
                bold: True






